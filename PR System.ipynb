   "source": [
    "import pandas\n",
    "news = pandas.read_csv(\"latest_news.csv\")\n",
    "brands = pandas.read_excel(\"Brand_Characteristics.xlsx\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "names = brands['Brand name']\n",
    "\n",
    "def count(item):\n",
    "    return filter(lambda name: str(name) in str(item[1]), names)\n",
    "\n",
    "news[\"mentions\"] = news.apply(count, axis=1)"
   ]
  },
